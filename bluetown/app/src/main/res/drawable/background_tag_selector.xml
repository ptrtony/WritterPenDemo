<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true"><shape>
            <solid android:color="@color/compnay_show_type_tag_stroke" />

            <corners android:radius="90px" />

            <padding android:bottom="10dp" android:left="40dp" android:right="40dp" android:top="10dp" />
        </shape></item>
    <item><shape>
            <solid android:color="@color/white" />

            <stroke android:width="1dp" android:color="@color/compnay_show_type_tag_text_bg" />

            <corners android:radius="90px" />
            
            <padding android:bottom="10dp" android:left="40dp" android:right="40dp" android:top="10dp" />
        </shape></item>

</selector>